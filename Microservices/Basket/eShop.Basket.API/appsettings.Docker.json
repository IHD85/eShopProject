{
  "ConnectionStrings": {
    "BasketDb": "Host=eshop-db;Port=5432;Database=BasketDb;Username=postgres;Password=postgres"
  },
  "Redis": {
    "Host": "redis",
    "Port": 6379
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.File", "Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [{
      "Name": "Console",
      "Args": {
        "path": "logs/basket-logs.txt",
        "rollingInterval": "Day"
      }
    }],
    "Enrich": ["FromLogContext", "WithThreadId", "WithMachineName"]
    
    
  
},
  "RabbitMQ": {
    "Host": "rabbitmq",
    "User": "guest",
    "Password": "guest",
    "QueueName": "basket_queue"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
