{
  "ConnectionStrings": {
    "BasketDb": "Host=eshop-db;Port=5432;Database=BasketDb;Username=postgres;Password=postgres"
  },
  "Redis": {
    "Host": "redis",
    "Port": 6379
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/basket-logs.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "WithExceptionDetails",
      "WithMachineName"
    ]
  }
,
  "RabbitMQ": {
    "Host": "rabbitmq",
    "User": "guest",
    "Password": "guest",
    "QueueName": "basket_queue"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
