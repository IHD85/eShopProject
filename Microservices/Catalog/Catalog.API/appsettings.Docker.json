{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=eshop-db;Port=5432;Database=eshop_catalog;Username=postgres;Password=postgres"
  },
  "RabbitMQ": {
    "Host": "rabbitmq",
    "User": "guest",
    "Password": "guest",
    "QueueName": "catalog_queue"
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.File", "Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [{
      "Name": "Console",
      "Args": {
        "path": "logs/catalog-logs.txt",
        "rollingInterval": "Day"
      }
    }],
    "Enrich": ["FromLogContext", "WithThreadId", "WithMachineName"]
  }


}
